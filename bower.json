{
  "name": "curriculumvitae",
  "private": true,
  "dependencies": {
		"bootstrap": "3.3.6",
		"jquery": "1.11.2",
		"wow": "1.0.2",
		"jquery.stellar": "0.6.2",
		"bootstrap-social": "4.8.0",
		"jquery.scrollTo": "1.4.14",
		"owl-carousel": "1.3.2",
		"font-awesome": "4.3.0",
		"smoothscroll": ""
	},
	"overrides": {
		"bootstrap": {
			"main": [
				"dist/js/*.*",
				"dist/css/*.*",
				"dist/fonts/*.*"
			]
		},
		"wow": {
			"main":[
				"css/libs/*.*",
				"dist/*.*"
			]
		},
		"font-awesome": {
			"main":[
				"css/font-awesome.css",
				"fonts/*.*" 
			]
		},
		"owl-carousel": {
			"main":[
				"owl-carousel/*.*"
			]
		}
	}
}
